    "gulp-watch": "^3.0.0",
    "jasmine-core": "^2.1.3",
    "karma": "^0.12.28",
    "karma-chrome-launcher": "^0.1.7",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.3.2",
    "nodemon": "^1.2.1",
    "should": "^4.4.1",
    "supertest": "^0.15.0"
  },
  "dependencies": {
    "archiver": "^0.13.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^2.4.0",
