{
  "name": "CodeFriends",
  "version": "1.0.0",
  "description": "Remote pair/group programming environment",
  "main": "client/nw.html",
  "scripts": {
    "start": "./node_modules/forever/bin/forever start -p ./data/logs -a -o ./data/logs/stdout.log -e ./data/logs/stderr.log server ",
    "test": "export NODE_ENV='test'; ./node_modules/mocha/bin/mocha -t 15000 server/tests/index",
    "dev": "./node_modules/nodemon/bin/nodemon.js --watch ./server server",
    "clean": "npm install && bower install && node server/deleteAllDatabases && gulp",
    "clean-test": "export NODE_ENV='test'; node server/deleteAllDatabases"
  },
  "window": {
    "toolbar": true,
    "frame": true,
    "width": 800,
    "height": 600
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-friends/CodeFriends.git"
  },
  "keywords": [
    "programming",
    "environment",
    "pair",
    "group",
    "chat",
    "video"
  ],
  "author": "CodeFriends",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-friends/CodeFriends/issues"
  },
  "homepage": "https://github.com/code-friends/CodeFriends",
